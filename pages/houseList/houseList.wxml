              </view>
            </block>
          </view>
        </view>
        <view class="bnblist-listpop-filter-flex-2 J_b">
          <view class="bnblist-listpop-filter bnblist-listpop-filter-2 J_item bnblist-listpop-filter-hasfold">
            <view class="tit ">{{filter_list.filters[1].text}}</view>
