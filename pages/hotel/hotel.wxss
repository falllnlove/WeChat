}
.bnbbook-box-date .tit {
  font-family: 'Microsoft YaHei UI Light', 'PingFangSC-Light', Helvetica, 'Hiragino Sans GB W3', 'Microsoft YaHei', arial, sans-serif-light, sans-serif;
  font-weight: 200;
  font-size: 0.6rem;
  color: #999;
  letter-spacing: 0;
  text-align: left;
  line-height: 0.6rem;
  display: block;
  margin-bottom: 0.15rem;
}
.bnbbook-box-date .txt-date {
  font-weight: 400;
  font-size: 1.1rem;
  color: #3c3c3c;
  text-align: left;
  line-height: 1.25rem;
  vertical-align: middle;
}
.bnbbook-box-date .txt-day {
  font-size: 0.6rem;
  color: #3c3c3c;
  text-align: left;
  line-height: 1.25rem;
  vertical-align: middle;
}
.bnbbook-box-date .con-day {
  position: relative;
}
.bnbbook-box-date .con-day .txt {
  border: none;
  background: #fff;
  min-width: 2rem;
  height: 0.85rem;
  line-height: 0.30rem;
  display: inline-block;
  text-align: center;
  margin-top: 1rem;
  padding-left: 0.5rem;
  padding-right: 0.5rem;
  box-sizing: border-box;
  position: relative;
  z-index: 2;
  color: #3c3c3c;
}
.bnbbook-box-date .con-day .txt::after {
  border-radius: 0.85rem;
  border-color: #ddd;
  width: 198%;
  height: 198%;
  top: -0.05rem;
}
.bnbbook-box-date .con-day .txt::after {
  content: '';
  position: absolute;
  left: 0;
  border: 1px solid #e9e9e9;
  box-sizing: border-box;
  width: 200%;
  height: 200%;
  transform: scale(0.5);
  transform-origin: left top;
}
