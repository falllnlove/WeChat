  overflow: hidden;
}
.bnb-modal .box-body {
  padding: 1rem 0.75rem;
}
.bnb-modal.bnblbook-modal-overseas .box-body::after {
  content: '';
  background-image: linear-gradient(-180deg, rgba(255, 255, 255, 0) 0%, #fff 71%);
