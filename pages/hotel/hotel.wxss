  left: 0;
  right: 0;
  top: 0;
  box-sizing: border-box;
  opacity: 1;
}
.bnb-modal.bnblbook-modal-overseas .box-head .tit {
  padding-top: 3rem;
  text-align: center;
  font-size: 0.8rem;
}
.bnb-modal.bnblbook-modal-overseas .box-body {
  margin-top: 4rem;
  padding-left: 0.75rem;
  padding-right: 0.75rem;
  font-size: 0.7rem;
