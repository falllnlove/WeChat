/*酒店信息*/
view{
  display: block;
}
image{
  vertical-align: middle;
}
.q{
  width: 100%;
  height: 1px;
  background-color: #f5f5f5;
}
.bnbbook-box-bookinfo{
  overflow: hidden;
  width: 90%;
  margin: 0 auto;
  margin-top: 2%;
}
.bnbbook-box-bookinfo .con-img{
  position: relative;
  float: left;
}
.bnbbook-box-bookinfo .con-img::after{
  content: '';
  display: block;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  border-radius: 0.15rem;
  background: rgba(255, 255, 255, 0);
}
.bnbbook-box-bookinfo .con-img image{
  display: block;
  width: 5rem;
  height: 3.75rem;
  border-radius: 0.15rem;
}
.btn-o{
  float: right;
  font-size: 0.6rem;
  line-height: 0.7rem;
  margin-top: 1.6rem;
  position: relative;
  padding-right: 1rem;
  color: #999;
}
.btn-o::after{
  content: '';
  display: block;
  background-image: url(http://pic.c-ctrip.com/h5/bnb/ico-01.v6.png?v=1);
  background-size: 8.1rem 7.4rem;
  background-repeat: no-repeat;
  background-position: -7.6rem -3.6rem;
  width: 0.3rem;
  height: 0.6rem;
  position: absolute;
  right: 0;
  top: 0;
  margin-top: 0.05rem;
  padding-right: 0.5rem;
}
.bnbbook-box-bookinfo .tit{
  display: -webkit-box;
  overflow: hidden;
  font-size: 0.8rem;
  line-height: 0.9rem;
  font-weight: 700;
  color: #3c3c3c;
  margin-left: 5.5rem;
  margin-right: 2.5rem;
}
.bnbbook-box-bookinfo .con-txt{
  font-size: 0.6rem;
  color: #666;
  margin-top: 0.5rem;
  margin-left: 5.5rem;
  margin-right: 2.5rem;
}
