/* 境外人士提示begin */
.bnb-modal.is-hide {
  display: block!important;
  visibility: hidden;
}
.bnb-modal {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  opacity: 1;
}
.bnb-modal::before {
  content: '';
  display: block;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.5);
  opacity: 1;
}
.bnb-modal.bnblbook-modal-overseas .modal-box {
  margin-top: 0;
}
.bnb-modal .modal-box {
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: #fff;
  position: fixed;
  opacity: 1;
}
.bnb-modal.bnblbook-modal-overseas .box-head {
  font-weight: 400;
  padding-top: 0;
}
.bnb-modal.bnblbook-modal-overseas .box-head {
  text-align: center;
  font-size: 1rem;
  line-height: 1rem;
  color: #3c3c3c;
